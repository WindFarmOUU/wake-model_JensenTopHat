# For OUU wind farm
environment
    tabular_data

method
    polynomial_chaos
quadrature_order  20
    normalized
    output verbose

model
    single

variables
    uniform_uncertain = 1
lower_bounds = 0.0
upper_bounds = 330.0
    descriptors = 'x'

interface
    id_interface = 'UQ_INTERFACE'
    fork
        analysis_drivers = 'getPower.py'
        parameters_file = 'params.in'
        results_file = 'results.out'
        file_save

responses
    num_response_functions = 1
    no_gradients
    no_hessians
    descriptors = 'power'


